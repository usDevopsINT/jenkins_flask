pipeline {
    agent any
    
    stages {
        stage('cloning...') {
            steps {
                git branch: 'main', url: 'https://github.com/usDevopsINT/jenkins_flask'
            }
        }
        stage('build...'){
            steps{
                sh 'cd flaskandjenkins'
                sh 'ls'
                sh 'python3 flask3.py &'
            }
        }
        stage('testing'){
            steps{
                echo "testing"
            }
        }
    }
}
